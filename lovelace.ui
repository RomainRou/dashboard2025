                if (entity && entity.state === "hail") return "url('https://i.gifer.com/DJxO.gif') no-repeat center center / cover";
                if (entity && entity.state === "partlycloudy") return "url('https://i.gifer.com/g1vA.gif') no-repeat center center / cover";
