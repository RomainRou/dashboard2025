                    state_not: unavailable
                chip:
                  type: template
                  tap_action:
                    action: none
                  hold_action:
                    action: none
                  double_tap_action:
                    action: none
                  icon: >-
                    {% set position =
                    states['[[cover_group]]'].attributes.current_position |
                    float(default=0) | int %} {% if position == 100 %}
                      cil:shutter-0
                    {% elif position >= 75 %}
                      cil:shutter-1
                    {% elif position >= 50 %}
                      cil:shutter-2
                    {% elif position >= 25 %}
                      cil:shutter-3
                    {% elif position >= 0 %}
                      cil:shutter-4
                    {% else %}
                      mdi:window-shutter-closed
