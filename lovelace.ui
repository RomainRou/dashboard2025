                    is_state('[[light_3_right]]', 'recording') %}
                     #03a9f4
                    {% endif %}
                  content: '[[content_light_3_right]]'
              - type: conditional
                conditions:
                  - condition: state
                    entity: '[[switch_camera_ext_3]]'
                    state:
                      - 'on'
                chip:
                  type: template
                  tap_action:
                    action: navigate
                    navigation_path: '[[navigation_cam_3]]'
                  hold_action:
                    action: none
                  double_tap_action:
                    action: none
                  entity: '[[entity_camera_ext_3]]'
                  icon: >
                    {% if states['[[entity_camera_ext_3]]'].attributes['icon']
                    %} {{ states['[[entity_camera_ext_3]]'].attributes['icon']
                    }} {% elif
                    states['[[entity_camera_ext_3]]'].attributes['device_class']
                    %}
                      mdi:{{ states['[[entity_camera_ext_3]]'].attributes['device_class'] }}
                    {% else %}
                      mdi:device-unknown
                    {% endif %}
                  icon_color: '[[entity_color_camera]]'
                  content: '[[content_camera_ext_3]]'
              - type: conditional
                conditions:
                  - condition: state
                    entity: '[[switch_camera_ext_4]]'
                    state:
                      - 'on'
                chip:
                  type: template
                  tap_action:
                    action: navigate
                    navigation_path: '[[navigation_cam_4]]'
                  hold_action:
                    action: none
